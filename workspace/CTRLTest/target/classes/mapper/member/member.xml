<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcwk.ctrl.member">
     
   <insert id="doMemberInsert" parameterType="com.pcwk.ctrl.cmn.MemberVO">
     INSERT INTO member (  
			m_num,
			m_name,
			m_email,
			m_tel,
			m_addr,
			m_grade     
     ) VALUES (            
         #{mNum}, 
         #{mName}, 
         #{mEmail}, 
         #{mTel}, 
         #{mAddr}, 
         #{mGrade} 
     )    
   </insert>  


</mapper>